{
    "manifest_version" : 2,
    "name" : "myBackground",
    "description" : "Add a customm background to Google.com",
    "version" : "1.0",
    "icons" : {
        "28" : "/images/icon-28.png",
        "48" : "/images/icon-48.png",
        "128" : "/images/icon-120.png"
    },
    "page_action" : {
        "default_icon" : {
            "28" : "/images/icon-28.png",
            "48" : "/images/icon-48.png",
            "128" : "/images/icon-120.png"
        }
    },
    "content_scripts" : [
        {
            "matches" : ["https://wwww.google.com/*"],
            "css" : ["main.css"]
        }
    ]
}